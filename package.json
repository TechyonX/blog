{
  "name": "techyon-blog",
  "version": "1.0.0",
  "main": "src/pages/index.tsx",
  "repository": "https://github.com/TechyonX/blog",
  "author": "TechyonX",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint ./src/ --ext .tsx,.ts",
    "lint-style": "stylelint ./src/**/*.scss --fix",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "yarn run prettier --write"
    ],
    "*.scss": "yarn lint-style"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.16",
    "@mdx-js/react": "^1.6.16",
    "gatsby": "^2.23.7",
    "gatsby-image": "^2.4.14",
    "gatsby-plugin-html-attributes": "^1.0.5",
    "gatsby-plugin-manifest": "^2.4.29",
    "gatsby-plugin-mdx": "^1.2.34",
    "gatsby-plugin-offline": "^3.2.28",
    "gatsby-plugin-react-head": "^1.1.0",
    "gatsby-plugin-sharp": "^2.6.25",
    "gatsby-remark-autolink-headers": "^2.3.11",
    "gatsby-remark-images-anywhere": "^1.3.0",
    "gatsby-remark-prismjs": "^3.5.11",
    "gatsby-source-strapi": "^0.0.12",
    "gatsby-transformer-sharp": "^2.5.12",
    "prismjs": "^1.21.0",
    "prismjs-material-theme": "^1.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-head": "^3.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "eslint": "^7.3.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "foundation-sites": "^6.6.3",
    "gatsby-plugin-sass": "^2.3.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "stylelint": "^13.6.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^3.9.5"
